{
  "build": {
    "core": "esp32",
    "mcu": "esp32s3",
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "arduino": {
      "ldscript": "esp32s3_out.ld",
      "partitions": "default_16MB.csv"
    },
    "extra_flags": [
      "-DARDUINO_TWATCH_S3",
      "-DARDUINO_USB_MODE=1",
      "-DARDUINO_USB_CDC_ON_BOOT=1"
    ]
  },
  "connectivity": ["wifi", "bluetooth", "ble"],
  "debug": { "openocd_target": "esp32s3.cfg" },
  "frameworks": ["arduino"],
  "name": "LilyGo T-Watch S3",
  "upload": {
    "maximum_size": 16777216,
    "maximum_ram_size": 327680,
    "require_upload_port": true,
    "speed": 921600
  },
  "url": "https://github.com/Xinyuan-LilyGO/T-Watch-S3",
  "vendor": "LilyGo"
}
