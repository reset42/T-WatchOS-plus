{
  "name": "LILYGO T-Watch S3 (16MB Flash, 8MB PSRAM OPI)",
  "url": "https://github.com/Xinyuan-LilyGO/LilyGoLib",
  "vendor": "LILYGO",
  "build": {
    "core": "esp32",
    "cpu": "esp32s3",
    "f_cpu": "240000000L",
    "mcu": "esp32s3",
    "variant": "esp32s3",

    "flash_mode": "qio",
    "f_flash": "80000000L",

    "arduino": { "memory_type": "qio_opi" },
    "extra_flags": "-DARDUINO_TWATCH_S3"
  },
  "connectivity": ["wifi", "bluetooth"],
  "debug": { "tool": "esp-builtin", "openocd_target": "esp32s3.cfg" },
  "frameworks": ["arduino"],
  "upload": {
    "flash_size": "16MB",
    "maximum_size": 16777216,
    "maximum_ram_size": 327680,
    "require_upload_port": true,
    "speed": 921600
  }
}
